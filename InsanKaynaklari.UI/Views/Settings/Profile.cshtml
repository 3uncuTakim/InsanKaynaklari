@model EmployeeProfileVM
@{
    ViewData["Title"] = "Profile";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class="table table-hover">
    <thead>
        <tr>
            <td>FirstName</td>
            <td>LastName</td>
            <td>Birthday</td>
            <td>Adress</td>
            <td>Title</td>
            <td>Department</td>
            <td>Workstyle</td>
            <td>StartDate</td>
            <td>Picture</td>
        </tr>

    </thead>
    <tbody>
        <tr>
            <td>@Model.FirstName</td>
            <td>@Model.LastName</td>
            <td>@Model.Birthday</td>
            <td>@Model.Address</td>
            <td>@Model.Title</td>
            <td>@Model.Department</td>
            <td>@Model.WorkStyle</td>
            <td>@Model.StartDate</td>
            <td>@Model.Picture</td>

        </tr>
    </tbody>
</table>

